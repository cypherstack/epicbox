[package]
name = "stack_test_epicbox"
version = "0.1.1"
authors = ["vault713 devs"]
description = "A slate relay service for Epic."
license = "Apache-2.0"
repository = "https://github.com/EpicCash/epicbox"
readme = "README.md"

[dependencies]
bytes = "0.4"
clap = "2.31"
colored = "1.6"
env_logger = "0.6"
failure = "0.1"
futures = "0.1"
log = "0.4"
nitox = "0.1"
nom = "4.2"
serde = "1"
serde_derive = "1"
serde_json = "1.0"
tokio = "0.1"
tokio-codec = "0.1"
tokio-core = "0.1"
tokio-io = "0.1"
tokio-timer = "0.2"
toml = "0.4"
unicode-segmentation = "0.1"
uuid = { version = "0.7", features = ["serde", "v4"] }
ws = "0.7"

stack_test_epicboxlib = "1.0.2"
