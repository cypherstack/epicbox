[package]
name = "stack_test_epicboxlib"
version = "1.0.3"
authors = ["vault713 devs"]
description = "A library that encapsulates epicbox integration."
license = "Apache-2.0"
repository = "https://github.com/EpicCash/epicbox"
edition = "2018"

[dependencies]
colored = "1.7"
failure = "0.1"
log = "0.4"
parking_lot = {version = "0.6"}
rand = "0.5"
regex = "1"
ring = "0.13"
serde = "1.0"
serde_derive = "1.0"
serde_json = "1.0"
sha2 = "0.7"
ws = { version="0.7", features=["ssl"] }

grin_secp256k1zkp = { version = "0.7.4", features = ["bullet-proof-sizing"]}

stack_test_epic_core = "3.0.0"
stack_test_epic_wallet_libwallet = "3.0.1"
